<?xml version="1.0"?>
<config>
    <modules>
        <Hycubecompleteplus_Autosuggest>
            <version>3.4.4</version>
            <url>http://hycubecompleteplus.com/</url>
            <modulename>Hycubecompleteplus_Autosuggest</modulename>
        </Hycubecompleteplus_Autosuggest>
    </modules>
    <global>
        <models>
            <hycubecompleteplus_autosuggest>
                <class>Hycubecompleteplus_Autosuggest_Model</class>
                <resourceModel>hycubecompleteplus_autosuggest_resource</resourceModel>
            </hycubecompleteplus_autosuggest>
            <hycubecompleteplus_autosuggest_resource>
                <class>Hycubecompleteplus_Autosuggest_Model_Resource</class>
                <deprecatedNode>hycubecompleteplus_mysql4</deprecatedNode>
                <entities>
                    <notifications>
                        <table>hycubecompleteplus_notifications</table>
                    </notifications>
                    <pusher>
                        <table>hycubecompleteplus_pusher</table>
                    </pusher>
                    <config>
                        <table>hycubecompleteplus_config</table>
                    </config>
                    <batches>
                        <table>hycubecompleteplus_batches</table>
                    </batches>
                </entities>
            </hycubecompleteplus_autosuggest_resource>
            <catalogsearch_resource>
                <rewrite>
                    <fulltext_collection>Hycubecompleteplus_Autosuggest_Model_Resource_Fulltext_Collection</fulltext_collection>
                </rewrite>
            </catalogsearch_resource>
        </models>
        <events>
            <controller_action_postdispatch_checkout_cart_index>
                <observers>
                    <hycubecompleteplus_autosuggest>
                        <class>hycubecompleteplus_autosuggest/observer</class>
                        <method>webhook_service_call</method>
                    </hycubecompleteplus_autosuggest>
                </observers>
            </controller_action_postdispatch_checkout_cart_index>
            <controller_action_postdispatch_checkout_onepage_index>
                <observers>
                    <hycubecompleteplus_autosuggest>
                        <class>hycubecompleteplus_autosuggest/observer</class>
                        <method>webhook_service_call</method>
                    </hycubecompleteplus_autosuggest>
                </observers>
            </controller_action_postdispatch_checkout_onepage_index>
            <controller_action_postdispatch_checkout_onepage_success>
                <observers>
                    <hycubecompleteplus_autosuggest>
                        <class>hycubecompleteplus_autosuggest/observer</class>
                        <method>webhook_service_call</method>
                    </hycubecompleteplus_autosuggest>
                </observers>
            </controller_action_postdispatch_checkout_onepage_success>
            <controller_isp_custom_onepage_success>
                <observers>
                    <hycubecompleteplus_autosuggest>
                        <class>hycubecompleteplus_autosuggest/observer</class>
                        <method>webhook_service_call</method>
                    </hycubecompleteplus_autosuggest>
                </observers>
            </controller_isp_custom_onepage_success>
            <catalog_product_save_before>
                <observers>
                    <hycubecompleteplus_autosuggest>
                        <type>singleton</type>
                        <class>hycubecompleteplus_autosuggest/observer</class>
                        <method>catalog_product_save_after</method>
                    </hycubecompleteplus_autosuggest>
                </observers>
            </catalog_product_save_before>
            <isp_catalog_product_save_before>
                <observers>
                    <hycubecompleteplus_autosuggest>
                        <type>singleton</type>
                        <class>hycubecompleteplus_autosuggest/observer</class>
                        <method>catalog_product_save_after</method>
                    </hycubecompleteplus_autosuggest>
                </observers>
            </isp_catalog_product_save_before>
            <catalog_product_save_after>
                <observers>
                    <hycubecompleteplus_autosuggest>
                        <type>singleton</type>
                        <class>hycubecompleteplus_autosuggest/observer</class>
                        <method>catalog_product_save_after_real</method>
                    </hycubecompleteplus_autosuggest>
                </observers>
            </catalog_product_save_after>
            <catalog_product_delete_before>
                <observers>
                    <hycubecompleteplus_autosuggest>
                        <type>singleton</type>
                        <class>hycubecompleteplus_autosuggest/observer</class>
                        <method>catalog_product_delete_before</method>
                    </hycubecompleteplus_autosuggest>
                </observers>
            </catalog_product_delete_before>
            <catalog_product_import_finish_before>
                <observers>
                    <hycubecompleteplus_autosuggest>
                        <type>singleton</type>
                        <class>hycubecompleteplus_autosuggest/observer</class>
                        <method>catalog_product_import_finish_before</method>
                    </hycubecompleteplus_autosuggest>
                </observers>
            </catalog_product_import_finish_before>
        </events>
        <resources>
            <autosuggest_setup>
                <setup>
                    <module>Hycubecompleteplus_Autosuggest</module>
                </setup>
                <connection>
                    <use>core_setup</use>
                </connection>
            </autosuggest_setup>
            <autosuggest_write>
                <connection>
                    <use>core_write</use>
                </connection>
            </autosuggest_write>
            <autosuggest_read>
                <connection>
                    <use>core_read</use>
                </connection>
            </autosuggest_read>
        </resources>
        <blocks>
            <hycubecompleteplus_autosuggest>
                <class>Hycubecompleteplus_Autosuggest_Block</class>
            </hycubecompleteplus_autosuggest>
        </blocks>
        <template>
            <email>
                <autosuggest_status_notification module="hycubecompleteplus_autosuggest">
                    <label>Hycubecompleteplus status notification</label>
                    <file>hycubecompleteplus/status_notification.html</file>
                    <type>html</type>
                </autosuggest_status_notification>
            </email>
        </template>
        <helpers>
            <hycubecompleteplus_autosuggest>
                <class>Hycubecompleteplus_Autosuggest_Helper</class>
            </hycubecompleteplus_autosuggest>
            <catalogsearch>
                <rewrite>
                    <data>Hycubecompleteplus_Autosuggest_Helper_Catalogsearch</data>
                </rewrite>
            </catalogsearch>
        </helpers>
        <sales>
            <quote>
                <item>
                    <product_attributes>
                        <added_from_search />
                    </product_attributes>
                </item>
            </quote>
        </sales>
        <rewrite>
            <fancy_url>
                <from><![CDATA[/instantsearchplus\/result\/(.*)/]]></from>
                <to><![CDATA[/instantsearchplus/result/index/]]></to>
                <complete>1</complete>
            </fancy_url>
        </rewrite>
    </global>
    <frontend>
        <layout>
            <updates>
                <autosuggest>
                    <file>hycubecompleteplus.xml</file>
                </autosuggest>
            </updates>
        </layout>
        <routers>
            <instantsearchplus>
                <use>standard</use>
                <args>
                    <module>Hycubecompleteplus_Autosuggest</module>
                    <frontName>instantsearchplus</frontName>
                </args>
            </instantsearchplus>
            <hycubecompleteplus>
                <use>standard</use>
                <args>
                    <module>Hycubecompleteplus_Autosuggest</module>
                    <frontName>hycubecompleteplus</frontName>
                </args>
            </hycubecompleteplus>
            <catalogsearch>
                <args>
                    <modules>
                        <Hycubecompleteplus_Autosuggest before="Mage_CatalogSearch">Hycubecompleteplus_Autosuggest_CatalogSearch</Hycubecompleteplus_Autosuggest>
                    </modules>
                </args>
            </catalogsearch>
        </routers>
    </frontend>
    <admin>
        <routers>
            <adminhtml>
                <args>
                    <modules>
                        <hycubecompleteplus after="Mage_Adminhtml">Hycubecompleteplus_Autosuggest_Adminhtml</hycubecompleteplus>
                    </modules>
                </args>
            </adminhtml>
        </routers>
    </admin>
    <adminhtml>
        <layout>
            <updates>
                <autosuggest>
                    <file>hycubecompleteplus.xml</file>
                </autosuggest>
            </updates>
        </layout>
    </adminhtml>
    <default>
        <hycubecompleteplus>
            <config>
                <attributes>1</attributes>
                <layered>0</layered>
                <api_endpoint><![CDATA[https://magento.instantsearchplus.com]]></api_endpoint>
            </config>
        </hycubecompleteplus>
    </default>
</config>
